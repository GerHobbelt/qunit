{
  "name": "@gerhobbelt/qunit",
  "title": "QUnit",
  "description": "An easy-to-use JavaScript Unit Testing framework.",
  "version": "2.6.1-5",
  "homepage": "https://qunitjs.com",
  "author": {
    "name": "jQuery Foundation and other contributors",
    "url": "https://github.com/qunitjs/qunit/blob/2.6.1/AUTHORS.txt"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/qunitjs/qunit.git"
  },
  "keywords": [
    "testing",
    "unit",
    "jquery"
  ],
  "bugs": {
    "url": "https://github.com/qunitjs/qunit/issues"
  },
  "license": "MIT",
  "bin": {
    "qunit": "bin/qunit"
  },
  "files": [
    "bin/",
    "qunit/qunit.js",
    "qunit/qunit.css",
    "LICENSE.txt"
  ],
  "dependencies": {
    "chokidar": "2.0.4",
    "commander": "2.16.0",
    "exists-stat": "1.0.0",
    "findup-sync": "2.0.0",
    "js-reporters": "1.2.1",
    "resolve": "1.8.1",
    "walk-sync": "0.3.2"
  },
  "devDependencies": {
    "async": "2.6.1",
    "babel-core": "6.26.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.7.0",
    "browserstack-runner": "0.7.0",
    "co": "4.6.0",
    "commitplease": "3.2.0",
    "eslint-config-jquery": "1.0.1",
    "eslint-plugin-html": "4.0.5",
    "eslint-plugin-qunit": "3.2.1",
    "execa": "0.10.0",
    "fixturify": "0.3.4",
    "fs-extra": "6.0.1",
    "grunt": "1.0.3",
    "grunt-cli": "1.2.0",
    "grunt-concurrent": "2.3.1",
    "grunt-contrib-connect": "1.0.2",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-qunit": "2.0.0",
    "grunt-contrib-watch": "1.1.0",
    "grunt-coveralls": "2.0.0",
    "grunt-eslint": "21.0.0",
    "grunt-git-authors": "3.2.0",
    "grunt-istanbul": "0.8.0",
    "grunt-rollup": "9.0.0",
    "grunt-search": "0.1.8",
    "load-grunt-tasks": "4.0.0",
    "npm-reporter": "file:./test/cli/fixtures/npm-reporter",
    "proxyquire": "2.0.1",
    "requirejs": "2.3.5",
    "rollup-plugin-babel": "3.0.7",
    "semver": "5.5.0"
  },
  "scripts": {
    "browserstack": "grunt build && sh build/run-browserstack.sh",
    "build": "grunt build",
    "dev": "grunt watch",
    "test": "grunt",
    "coverage": "grunt coverage",
    "test:cli": "grunt build && npm link && qunit test/cli/*.js",
    "pub": "npm publish --access public"
  },
  "commitplease": {
    "components": [
      "All",
      "Assert",
      "Build",
      "CLI",
      "CSS",
      "Core",
      "Docs",
      "Dump",
      "HTML Reporter",
      "Readme",
      "Test",
      "Tests"
    ]
  },
  "main": "qunit/qunit.js",
  "engines": {
    "node": ">=4"
  }
}
